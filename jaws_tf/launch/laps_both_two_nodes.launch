<launch>
  <param name="robot_description" textfile="$(find jaws_description)/urdf/jaws.urdf" />

  <node name="robot_state_publisher" pkg="robot_state_publisher" type="robot_state_publisher" />

  <node name="laps_tf" pkg="jaws_tf" type="laps_tf" />
  <node name="joint_pub" pkg="jaws_tf" type="joint_pub" />

  <node name="rviz" pkg="rviz" type="rviz" args="-d $(find jaws_rviz)/config/isometric-x5.rviz" required="true" />
</launch>
